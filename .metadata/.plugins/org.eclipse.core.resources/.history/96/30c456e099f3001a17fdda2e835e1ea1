 /** the program generates 10 random numbers
  * and will output the largest, smallest, and sum of the values*/

import java.util.Scanner;

public class problemTwo {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Here is your list of numbers: ");
		double max = 0;
		double min = 0;
		double randomNumber = Math.random();
		for (int i = 0; i < 5; i ++) {
			
			double previousNum = randomNumber;
			randomNumber = Math.random();
	
			
			//NOT WORKING!!!!
			if (previousNum > randomNumber) {
				max = previousNum;
				System.out.printf("%.3f \r\n", previousNum);
				System.out.print( "__" + max);
			}
			else if (previousNum < randomNumber) {
				min = previousNum;	
				System.out.printf("%.3f \r\n", previousNum);
				System.out.print("__" + min );
			}
		
		}
			
		System.out.printf("Max: %.3f \r\n", max);
		System.out.printf("Min: %.3f \r\n", min);
		
		in.close();
			}
		
		}
	

	