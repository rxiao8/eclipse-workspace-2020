 /** the program generates 10 random numbers
  * and will output the largest, smallest, and sum of the values*/

import java.util.Scanner;

public class problemTwo {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Here is your list of numbers: ");
		double max = 0;
		double min = 0;
		double newMax = 0;
		double newMin = 0;
		
		for (int i = 0; i < 5; i ++) {
			
			double nextNum = Math.random();
	
			
			//NOT WORKING!!!!
			if (nextNum > max) {
				max = nextNum;
					System.out.printf("%.3f \r\n", nextNum);
					System.out.println("\r\n");
					System.out.print( "__max" + max);
			}
			else {
				min = nextNum;
			}
			if (nextNum < min) {
				newMin = nextNum;
				if (min < newMin) {
					min = newMin;	
					System.out.printf("%.3f \r\n", nextNum);
					System.out.println("\r\n");
					System.out.print("__min" + newMin );
				}
			}
		
		}
			
		System.out.printf("Max: %.3f \r\n", max);
		System.out.printf("Min: %.3f \r\n", newMin);
		
		in.close();
			}
		
		}
	

	